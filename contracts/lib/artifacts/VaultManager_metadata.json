		"devdoc": {
			"details": "Manages interest accrual and distribution for loans. Handles registration, deregistration of vaults, and interest handling for each loan.",
			"kind": "dev",
			"methods": {
				"calculateClaimableInterest(address)": {
					"details": "Calculates based on the blocks elapsed since the start of interest accrual.",
					"params": {
						"pool": "The address of the loan pool."
					},
					"returns": {
						"_0": "The amount of interest that can be claimed."
					}
				},
				"deregisterVault(address)": {
					"params": {
						"pool": "The address of the loan pool."
					}
				},
				"distributeInterest(address,address)": {
					"params": {
						"pool": "The address of the loan pool.",
						"to": "The recipient of the interest payment."
					}
				},
				"handleDefault(address,address)": {
					"params": {
						"pool": "The address of the loan pool which has defaulted."
					}
				},
				"refundRemainingInterest(address,address)": {
					"params": {
						"borrower": "The recipient of the remaining funds.",
						"pool": "The address of the loan pool."
					}
				},
				"registerPoolVault(address)": {
					"details": "Sets up the interest accrual infrastructure when a new loan is activated.",
					"params": {
						"pool": "The address of the loan pool."
					}
				}
			},
			"title": "VaultManager",
			"version": 1
		},
		"userdoc": {
			"kind": "user",
			"methods": {
				"calculateClaimableInterest(address)": {
					"notice": "Calculates the claimable interest for a pool at the current block."
				},
				"deregisterVault(address)": {
					"notice": "Deregisters a vault when a loan is closed and stops interest accrual."
				},
				"distributeInterest(address,address)": {
					"notice": "Distributes accrued interest to the lender if the loan is active."
				},
				"handleDefault(address,address)": {
					"notice": "Handles the default scenario by securing the collateral and distributing remaining interest."
				},
				"refundRemainingInterest(address,address)": {
					"notice": "Refunds all remaining tokens to the borrower, ensuring any unclaimed interest is left for the lender."
				},
				"registerPoolVault(address)": {
					"notice": "Registers a vault for a pool and begins interest accrual if the loan is active."
				}
			},
			"version": 1
		}
